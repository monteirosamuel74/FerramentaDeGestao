                    <asp:Button ID="btnRegistrarBiometria" runat="server" Style="display: none;" OnClick="btnRegistrarBiometria_OnClick" />

                    <script lang="JavaScript">
                        $(document).ready(function () {
                            $('#txtHoraMat').mask('99:99');
                            $('#txtHoraMatSai').mask('99:99');
                            $('#txtHoraVesp').mask('99:99');
                            $('#txtHoraVespSai').mask('99:99');
                            $('#txtDataJusti').mask('99/99/9999');
                        });

                        function readURL(input) {
                            if (input.files && input.files[0]) {
                                var reader = new FileReader();

                                reader.onload = function (e) {
                                    $(input).after('');
                                    $img = '<br><br><div style="margin-left:50px;background-image:url(' + e.target.result +
                                        '); width: 140px; height: 140px; background-size: 140px; background-position: top; background-position: center; border: 3px solid #CCC" class="img-circle"></div>';
                                    $(input).after($img);
                                }

                                reader.readAsDataURL(input.files[0]);
                            }
                        }

                        function RegistrarPonto(e) {
                            $("#hdd_tipo").val(e);
                            var event = document.createEvent('CustomEvent');
                            event.initCustomEvent("EnviarDadosString", true, true, { 'metodo': 'ExCadastroBiometriaUsuario' });
                            document.documentElement.dispatchEvent(event);
                            return false;
                        }

                        function ExRetornoBiometriaUsuario(retorno) {
                            var json = JSON.parse(retorno);
                            $("#hdd_chave").val(json.dados);

                            if ($("#hdd_tipo").val() === "1") {
                                $("#<%=btnRegistrarPonto.ClientID %>").click();
                            } else {
                                $("#<%=btnRegistrarBiometria.ClientID %>").click();
                            }
                        }

                        //input[type=file]
                        $('#<%=FileUpload1.ClientID%>').change(function () {
                            readURL(this);
                        });


                    </script>

