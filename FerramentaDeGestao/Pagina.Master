            function SalvaOrdemVersao(ordem_chamado, codigo_chamado) {
                //alert(codigo_chamado + '/' + ordem_chamado);
                var Dados = JSON.stringify({ codigo: codigo_chamado, ordem: ordem_chamado });

                $.ajax({
                    url: "Inicial.aspx/setOrdemVersao",
                    data: Dados,
                    type: "POST",
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function (retorno) {
                        if (ordem_chamado != "") {
                            //alert('Ordem Alterada com sucesso!');                           
                        }
                    },
                    error: function (req, status, error) {
                        //alert(error);
                        //alert(req.responseText);
                    }
                });
            }

            function AlterarModal() {
                $('#modalDados').modal('hide');
                $('#modalConfirma').modal('show');
                return false;
            }

            function ValidarSenha() {
                var senha = $("#TxtSenhaConfirmaHolerite").val();
                $.ajax({
                    url: "Inicial.aspx/ValidaPass",
                    data: "{ 'senha': '" + senha + "' }",
                    type: "POST",
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        var retorno = data.d;
                        if (retorno.indexOf("ok") >= 0) {
                            $("#divSenhaConfirma").hide(0);
                            $("#tableHolerites").fadeIn(200);
                        } else {
                            $("#LblSenhaIncorreta").fadeIn(150);
                        }
                    }
                });
            }

            function AumentaFonte() {
                var valor = $("#btnAumentaFonte").val();

                if (valor === "+") {
                    $("#modalMotivacao > .modal-dialog").css("width", "1200px");
                    $("#LblMensagem > p").css("font-size", "17px");
                    $("#btnAumentaFonte").val("-");
                } else {
                    $("#modalMotivacao > .modal-dialog").css("width", "900px");
                    $("#LblMensagem > p").css("font-size", "12px");
                    $("#btnAumentaFonte").val("+");
                }
            }

            var ajaxLoading = false;
            var intervalVerificaLogado = setInterval(verificaLogado, 15 * 1000);

            function verificaLogado() {
                if (ajaxLoading) return;

                ajaxLoading = true;
                $.ajax({
                    url: 'Default.aspx/sessionIsActive',
                    type: 'POST',
                    dataType: "json",
                    contentType: "application/json; charset=utf-8",
                    success: function (resp) {
                        ajaxLoading = false;
                        if (resp && resp.d) {
                            var obj = JSON.parse(resp.d);
                            if (!obj.response) {

                                const divTimer = "<div>ATENÇÃO: Sua sessão foi encerrada, você será redirecionado em <span id=\"timerRedirect\">10</span> segundos.</div>";

                                if (parseInt(obj.chamadosexec) > 0) {
                                    divTimer = "<div>ATENÇÃO: Sua sessão foi encerrada, você será redirecionado em <span id=\"timerRedirect\">10</span> segundos.<br><br>Existia chamado que estava em execução e sera pausado, não esqueça de retoma-lo.</div>";
                                }

                                setInterval(function () {
                                    const timerEl = document.getElementById("timerRedirect");
                                    let s = parseInt(timerEl.innerText);
                                    if (s == 0) {
                                        window.location.href = "Login.aspx";
                                        return;
                                    }

                                    timerEl.innerHTML = s - 1;
                                }, 1000);

                                swal({
                                    title: 'Atenção',
                                    text: divTimer,
                                    type: 'warning',
                                    html: true
                                });

                                clearInterval(intervalVerificaLogado);
                            }
                        }
                    },
                    error: function (resp) {
                        ajaxLoading = false;
                    }
                });
            }

            var secondsGamification = 15;
            var intervalGamification;

            function mostraGamification() {
                $('#modal-gamification').modal('show', { backdrop: 'static', keyboard: false });

                intervalGamification = setInterval(function () {
                    secondsGamification -= 1;
                    $("#secondsGamification").html(secondsGamification);
                    if (secondsGamification == 0) {
                        clearInterval(intervalGamification);

                        desabilitaEnviarGamification();
                        $("#divLoadingGamification").show();
                        $("#<%=btnGamificationExpirou.ClientID%>").click();
                    }
                }, 1000);

            }

            function enviarGamification() {
                clearInterval(intervalGamification);
                desabilitaEnviarGamification();
                $("#divLoadingGamification").show();
                $("#<%=btnGamification.ClientID%>").click();
            }

            function desabilitaEnviarGamification() {
                $("#btnEnviarGamification").removeClass("btn-success").addClass("btn-default")
                    .prop('disabled', true)
                    .html("Aguarde... <span class='glyphicon glyphicon-repeat normal-right-spinner'></span>");
            }

            function verificaCamposAlterarDados() {
                if ($("#<%=ddlSexo.ClientID%>").val() == "") {
                    alert('Selecione o sexo!');
                    $("#<%=ddlSexo.ClientID%>").focus();
                    return false;
                }

                return true;
            }
        </script>

