                    }
                });

                $(".lnkSenhaAcesso").on("click", function () {
                    //function copiarTextoDoElemento(elementoId, mostrarMensagem = true) {
                    var elemento;
                    if ("<%=lblSenhaAcesso.ClientID%>")
                        elemento = document.getElementById("<%=lblSenhaAcesso.ClientID%>");
                    else
                        elemento = event.target || event.srcElement;
                    var input = document.createElement("input");
                    input.style.border = "none";
                    input.style.background = "transparent";
                    input.style.color = "transparent";
                    input.style.bottom = "-9999px"
                    input.style.position = "fixed";
                    document.querySelector("body").appendChild(input);
                    input.value = elemento.innerText || element.value;
                    input.select();
                    document.execCommand("copy");
                    input.parentNode.removeChild(input);
                    //}
                });


                $(".lnkMensagemEntrega").on("click", function () {
                    //function copiarTextoDoElemento(elementoId, mostrarMensagem = true) {
                    var elemento;
