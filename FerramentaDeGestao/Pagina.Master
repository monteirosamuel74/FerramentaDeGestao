            <script>
                function getBusca() {

                    var txt = $("#<%=txtBusca.ClientID%>");
                    var DIV = $("#divContainer");

                    //alert(DIV.attr('class'));

                    if (txt.val() != "" || DIV.attr('class') == 'page-container sidebar-collapsed') {

                        if (DIV.attr('class') == 'page-container sidebar-collapsed') {
                            var txt2 = $("#<%=txtBusca2.ClientID%>");
                            $("#divBusca2").show();
                            txt2.focus();
                        }
                        else {
                            var campo = "<%=btnOCulto.ClientID %>";
                            document.getElementById(campo).click();
                        }
                    }
                    else {
                        alert('Favor informar o texto para busca!');
                    }
                }

                function getBusca2() {

                    var txt2 = $("#<%=txtBusca2.ClientID%>");
                    var txt = $("#<%=txtBusca.ClientID%>");

                    if (txt2.val() != "") {
                        txt.val(txt2.val());
                        var campo = "<%=btnOCulto2.ClientID %>";
                        document.getElementById(campo).click();
                    }
                    else {
                        alert('Favor informar o texto para busca!');
                    }
                }
            </script>

