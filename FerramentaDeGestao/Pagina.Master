                //Carrega imagens com lazy
                $(".miau").on("click", function () {
                    $('.lazy').lazy({
                        bind: "event",
                        delay: 0
                    });
                });

                $('#<%=txtBiografia.ClientID%>').on('keyup paste', function () {
                    limitText(this, 300);
                    CaracteresRestantesBiografia();
                });

                function limitText(field, maxChar) {
                    var ref = $(field),
                        val = ref.val();
                    if (val.length >= maxChar) {
                        ref.val(function () {
                            console.log(val.substr(0, maxChar))
                            return val.substr(0, maxChar);
                        });
                    }
                }

                CaracteresRestantesBiografia();
            });

            function CaracteresRestantesBiografia() {
                var maxLength = 300;
                $('#txtBiografiaCharsLeft').html(maxLength - $('#<%=txtBiografia.ClientID%>').val().length);
